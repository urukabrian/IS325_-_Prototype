{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "skipLibCheck": true,
    "noImplicitAny": false,
    "target": "es2017",
    "module": "commonjs",
    "jsx": "react",
    
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
    
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  
  
}

